     <script>
			var search = {};
			
			
			$('#submit').click(function() {
				search["pid"]=$('#pid').val();
				search["pname"] = $('#pname').val();
				search["clientname"] = $('#clientname').val();
				search["description"] = $('#description').val();
				search["duration"] =$('#duration').val();
				
				$.ajax({
					type : "POST",
					contentType : "application/json",
					url : "http://localhost:8080/Spring4Swagger/addproject",
					data : JSON.stringify(search),
					dataType : 'json',
					timeout : 100000,
					complete: function(data) {
						if(data)
							alert("SUCCESS");
						else
							alert("Fail");
					}
				});

			});
	</script>
